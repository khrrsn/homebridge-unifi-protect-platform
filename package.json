{
  "name": "homebridge-unifi-protect-platform",
  "version": "0.0.0",
  "description": "Homebridge platform for UniFi Protect with (Cameras, Doorbells).",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "homebridge-platform",
    "unifi",
    "doorbell",
    "camera",
    "category-security",
    "category-video",
    "category-hubs",
    "category-outdoor"
  ],
  "license": "MIT",
  "main": "lib/registerPlatform.js",
  "scripts": {
    "build": "rm -fr build && tsc",
    "commit": "git-cz",
    "dev": "ts-node-dev --respawn --transpile-only dev/start.ts",
    "dev:inspect": "ts-node --transpile-only dev/inspect.ts",
    "dev:stream": "ts-node-dev --respawn --transpile-only dev/stream.ts",
    "prepack": "yarn build",
    "release": "standard-version",
    "release:beta": "standard-version --prerelease beta"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "fetchit": "^3.0.0-beta.1",
    "homebridge-camera-ffmpeg": "^3.0.2",
    "rxjs": "^6.6.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.27",
    "@types/nopt": "^3.0.29",
    "@types/ws": "^7.2.6",
    "chalk": "^4.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "homebridge": "^1.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nopt": "^5.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-packagejson": "^2.2.5",
    "standard-version": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "homebridge": "^1.1.2"
  }
}
